<Window x:Class="ChampionshipSolutions.SelectEvent"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Select Event" Height="140" Width="500" ResizeMode="NoResize" ShowInTaskbar="False" Topmost="True">
    <Grid>
        <Button Content="OK" x:Name="btnOk" HorizontalAlignment="Left" Margin="409,70,0,0" VerticalAlignment="Top" Width="75" Height="31" Click="btnOk_Click"/>
        <Button Content="Cancel" x:Name="btnCancel" HorizontalAlignment="Left" Margin="10,70,0,0" VerticalAlignment="Top" Width="75" Height="31" Click="btnCancel_Click"/>
        <ComboBox x:Name="cbxSelectEvent" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Width="474" Height="46" SelectionChanged="ComboBox_SelectionChanged_1"/>

    </Grid>
</Window>
